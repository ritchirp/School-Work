public class test{
   public static void main(String[] args){
      int sum=0;
      int distance = 0;
      for(int i = 0; i<str.length(); i+=distance){
         distance = 0;
         if(str.charAt(i)!=' '){
            
            String num = "";
            for(int k = i; str.charAt(k)!=' '; k++){
               num+=str.charAt(k);
               distance++;
            }
            
            sum+=Integer.parseInt(num);
         }
      }
      return sum;
   }
}