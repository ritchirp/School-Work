import java.util.LinkedList;
import java.util.List;

public class Node {
	List<Integer> domain; 
	int value;
	List<Node> children;
	int row, col;
	
	public Node(){
		domain = new LinkedList<Integer>();
		for(int i=0; i<9; i++)
			domain.add(new Integer(i));
	}
	
	public Node(List dom, int val){
		domain = dom;
		value = val;
	}
	
}
