package edu.miamioh.ritchirp;

import javax.swing.JButton;

public class Tile extends JButton {
	private int row;
	private int column;
	private boolean hasQueen;
	
	public Tile(int row, int column){
		super();
		this.row = row;
		this.column = column;
	}
	
	public boolean hasQueen(){
		return hasQueen;
	}
}
